{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "es5",
    "module": "esnext",
    "jsx": "react",
    "moduleResolution": "Node10",
    "declaration": true,
    "isolatedModules": true,
    "noEmit": true,
    "outDir": "dist",
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
